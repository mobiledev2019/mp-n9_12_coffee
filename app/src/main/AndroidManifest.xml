<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.managercoffee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".UI.Income_overview"
            android:parentActivityName=".UI.dashbroad">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.managercoffee.UI.dashbroad" />
        </activity>
        <activity android:name=".UI.Day_overview"
            android:parentActivityName=".UI.Income_overview">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.managercoffee.UI.Income_overview" />
        </activity>
        <activity
            android:name=".UI.Shift"
            android:parentActivityName=".UI.dashbroad">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.managercoffee.UI.dashbroad" />
        </activity>
        <activity
            android:name=".UI.Item"
            android:parentActivityName=".UI.dashbroad">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.managercoffee.UI.dashbroad" />
        </activity>
        <activity
            android:name=".UI.Staff"
            android:parentActivityName=".UI.dashbroad">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.managercoffee.UI.dashbroad" />
        </activity>
        <activity android:name=".UI.dashbroad">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>